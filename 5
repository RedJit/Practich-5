using System;

namespace Practich5
{
    class Program
    {
        static void Main(string[] args)
        {
            int x = 0;
            string s = "";
            Console.Write("значение k = ");
            int k = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("последовательность натуральных чисел:");
            for (int i = 1; i <= k; i++)
            {

                x += 1;
                s += x;
            }
            Console.WriteLine(s);
            Console.WriteLine(k +"-я цифра " +   s[k - 1]);
            Console.ReadKey();
        }
    }
